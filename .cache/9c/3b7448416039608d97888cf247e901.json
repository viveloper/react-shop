lumn":8}},{"generated":{"line":71,"column":8},"source":"src/components/Breadcrumb.js","original":{"line":30,"column":6}},{"generated":{"line":71,"column":9},"source":"src/components/Breadcrumb.js","original":{"line":29,"column":4}},{"generated":{"line":72,"column":0},"source":"src/components/Breadcrumb.js","original":{"line":29,"column":4}},{"generated":{"line":73,"column":0},"source":"src/components/Breadcrumb.js","original":{"line":29,"column":4}},{"generated":{"line":73,"column":979},"source":"src/components/Breadcrumb.js","original":{"line":28,"column":2}},{"generated":{"line":74,"column":0},"source":"src/components/Breadcrumb.js","original":{"line":107,"column":1}},{"name":"Breadcrumb","generated":{"line":76,"column":15},"source":"src/components/Breadcrumb.js","original":{"line":109,"column":15}}],"sources":{"src/components/Breadcrumb.js":"import React, { useMemo } from 'react';\nimport { Link } from 'react-router-dom';\n\nfunction Breadcrumb({ pathName }) {\n  const pathList = [\n    {\n      id: 0,\n      to: '/home',\n      name: 'home',\n      displayName: 'home',\n    },\n    {\n      id: 1,\n      to: '/products',\n      name: 'products',\n      displayName: 'product list',\n    },\n    {\n      id: 2,\n      to: '/products/:id',\n      name: 'productDetail',\n      displayName: 'product detail',\n    },\n  ];\n\n  const currentPath = useMemo(() => pathList.find((path) => path.name === pathName), [pathName]);\n\n  return (\n    <>\n      <section className=\"breadcrumb-wrapper mb-4\">\n        <div className=\"container\">\n          <div className=\"row align-items-center\">\n            <div className=\"col-6\">\n              <div className=\"page-title\">\n                <h2>Product List</h2>\n              </div>\n            </div>\n            <div className=\"col-6\">\n              <ol className=\"breadcrumb justify-content-end align-items-center\">\n                {pathList\n                  .filter((path) => path.id <= currentPath.id)\n                  .map((path) =>\n                    path.name !== pathName ? (\n                      <li key={path.name} className=\"breadcrumb-item\">\n                        <Link to={path.to}>{path.displayName}</Link>\n                      </li>\n                    ) : (\n                      <li key={path.name} className=\"breadcrumb-item active\">\n                        {path.displayName}\n                      </li>\n                    )\n                  )}\n              </ol>\n            </div>\n          </div>\n        </div>\n      </section>\n\n      <style jsx>{`\n        /* breadcrumb */\n        .breadcrumb-wrapper {\n          padding-top: 108px;\n          background: #ffffff;\n        }\n        .breadcrumb-wrapper ol.breadcrumb {\n          background-color: transparent;\n          margin-bottom: 0;\n          padding: 0;\n        }\n        .breadcrumb-wrapper .page-title h2 {\n          color: #797979;\n          text-transform: uppercase;\n          font-weight: 400;\n          margin-bottom: 0;\n        }\n        .breadcrumb-wrapper ol.breadcrumb .active {\n          color: #252525;\n          text-transform: uppercase;\n        }\n        .breadcrumb-wrapper ol.breadcrumb li :global(a) {\n          text-transform: uppercase;\n          color: #969696;\n        }\n\n        @media (min-width: 480px) {\n          .breadcrumb-wrapper ol.breadcrumb {\n            float: right !important;\n            min-height: 70px;\n          }\n        }\n\n        @media (min-width: 480px) {\n          .breadcrumb-wrapper ol.breadcrumb li {\n            font-size: 9px;\n            line-height: 15px;\n          }\n        }\n\n        @media (min-width: 768px) {\n          .breadcrumb-wrapper ol.breadcrumb li {\n            font-size: 14px;\n          }\n        }\n      `}</style>\n    </>\n  );\n}\n\nexport default Breadcrumb;\n"},"lineCount":null}},"error":null,"hash":"2cb6330b90d5c8b80db5aae09d9c402b","cacheData":{"env":{}}}
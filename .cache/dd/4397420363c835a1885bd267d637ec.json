source":"src/pages/Home/Cart.js","original":{"line":20,"column":4}},{"generated":{"line":89,"column":224},"source":"src/pages/Home/Cart.js","original":{"line":19,"column":2}},{"generated":{"line":90,"column":0},"source":"src/pages/Home/Cart.js","original":{"line":70,"column":1}},{"name":"Cart","generated":{"line":92,"column":15},"source":"src/pages/Home/Cart.js","original":{"line":72,"column":15}}],"sources":{"src/pages/Home/Cart.js":"import React, { useMemo } from 'react';\r\nimport CartItem from './CartItem';\r\n\r\nfunction Cart({ items, onClose, onRemoveClick, onCheckout }) {\r\n  const totalPrice = useMemo(() => items.reduce((acc, item) => acc + item.price * item.count, 0), [items]);\r\n\r\n  const handleCloseBtnClicked = (e) => {\r\n    e.preventDefault();\r\n    e.stopPropagation();\r\n    onClose();\r\n  };\r\n\r\n  const handleCheckoutClick = (e) => {\r\n    e.preventDefault();\r\n    e.stopPropagation();\r\n    onCheckout({ items, totalPrice });\r\n  };\r\n\r\n  return (\r\n    <>\r\n      <div className=\"shopping-cart\">\r\n        <div className=\"cart-header text-center p-3\">\r\n          <a href=\"#\" className=\"drawer-close navy-link float-left\" onClick={handleCloseBtnClicked}>\r\n            <i className=\"fa fa-chevron-left fa-lg\" aria-hidden=\"true\"></i>\r\n          </a>\r\n          <span className=\"navy\">SHOPPING CART</span>\r\n          <div className=\"navy-line\"></div>\r\n        </div>\r\n        <div className=\"cart-content\">\r\n          <ul className=\"list-unstyled\">\r\n            {items.map((item) => (\r\n              <CartItem\r\n                key={item.id}\r\n                id={item.id}\r\n                name={item.name}\r\n                price={item.price}\r\n                count={item.count}\r\n                onRemoveClick={onRemoveClick}\r\n              />\r\n            ))}\r\n          </ul>\r\n          <div className=\"total-section px-3 py-4 gray-bg\">\r\n            <span>TOTAL:</span>\r\n            <span className=\"float-right m-0 price\">{totalPrice}</span>\r\n          </div>\r\n        </div>\r\n        <div className=\"cart-footer p-3\">\r\n          <a\r\n            className=\"btn btn-lg btn-primary checkout-btn\"\r\n            href=\"./checkout.html\"\r\n            role=\"button\"\r\n            onClick={handleCheckoutClick}>\r\n            Checkout\r\n          </a>\r\n        </div>\r\n      </div>\r\n      <style jsx>{`\r\n        .shopping-cart {\r\n          height: 100%;\r\n        }\r\n        .shopping-cart .total-section {\r\n          border-bottom: 1px solid rgba(0, 0, 0, 0.1);\r\n        }\r\n        .shopping-cart .checkout-btn {\r\n          width: 100%;\r\n        }\r\n      `}</style>\r\n    </>\r\n  );\r\n}\r\n\r\nexport default Cart;\r\n"},"lineCount":null}},"error":null,"hash":"6de8598da62d75eb3e36ae713f2cee4a","cacheData":{"env":{}}}